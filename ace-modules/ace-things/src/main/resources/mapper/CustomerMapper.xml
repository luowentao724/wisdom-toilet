<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.wxiaoqi.security.things.mapper.CustomerMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.github.wxiaoqi.security.things.entity.Customer" id="customerMap">
        <result property="id" column="id"/>
        <result property="customerName" column="customer_name"/>
        <result property="systemName" column="system_name"/>
        <result property="systemLogo" column="system_logo"/>
        <result property="contractName" column="contract_name"/>
        <result property="contractPhone" column="contract_phone"/>
        <result property="contractEmail" column="contract_email"/>
        <result property="remark" column="remark"/>
    </resultMap>

</mapper>